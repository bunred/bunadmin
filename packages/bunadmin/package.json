{
  "name": "@bunred/bunadmin",
  "version": "1.1.0-beta.1.4",
  "description": "A simple graphql admin dashboard. Easy to expand with the flexible plugin. Graphql first, also supports api. Based on React, Redux, Material-UI, Realtime RxDB, Formik, I18N.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bunred/bunadmin"
  },
  "bugs": {
    "url": "https://github.com/bunred/bunadmin/issues"
  },
  "author": "BunRed Team",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "pages",
    "public",
    "plugins",
    "src",
    "next.config.js",
    "next-env.d.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "setup": "yarn && yarn build",
    "dev": "next -p 1911",
    "build": "next build",
    "prettier": "prettier --write  --no-semi src/** pages/** plugins/** test/**",
    "start": "next start -p 1912",
    "test": "prettier --check --no-semi src/** pages/** test/** && jest",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "pack": "rm -rf dist & tsc && npm pack"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.54",
    "@mdx-js/loader": "^1.6.5",
    "@next/mdx": "^9.4.4",
    "@reduxjs/toolkit": "^1.3.6",
    "@types/mockjs": "^1.0.3",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-redux": "^7.1.9",
    "babel-loader": "^8.1.0",
    "bunadmin-auth-buncms": "^1.0.0-alpha.1",
    "clsx": "^1.1.1",
    "filehound": "^1.17.4",
    "formik": "^2.1.4",
    "formik-material-ui": "^2.0.0-beta.1",
    "i18next": "^19.4.5",
    "json-bigint": "^0.3.0",
    "lerna": "^3.22.0",
    "material-table": "npm:bunadmin-table@1.2.2",
    "mockjs": "^1.1.0",
    "next": "^9.4.4",
    "notistack": "^0.9.16",
    "pouchdb-adapter-http": "^7.2.1",
    "pouchdb-adapter-idb": "^7.2.1",
    "prism-react-renderer": "^1.1.1",
    "react": "latest",
    "react-dom": "latest",
    "react-eva-icons": "0.0.8",
    "react-i18next": "^11.5.0",
    "react-json-view": "^1.19.1",
    "react-redux": "^7.2.0",
    "rxdb": "8.9.0",
    "rxjs": "^6.5.4",
    "shortid": "^2.2.15",
    "umi-request": "^1.3.3"
  },
  "devDependencies": {
    "@bunred/sync-plugins": "^1.1.1",
    "@types/mdx-js__react": "^1.5.2",
    "@types/next": "latest",
    "@types/node": "^14.0.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "chalk": "^4.0.0",
    "husky": "^4.2.5",
    "ignore-loader": "^0.1.2",
    "jest": "^25.5.4",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "admin",
    "graphql",
    "react",
    "react admin",
    "material",
    "material admin",
    "plugin",
    "dashboard",
    "admin panel"
  ],
  "homepage": "https://www.bunred.com/"
}
