{
  "name": "@bunred/bunadmin",
  "version": "1.1.1-beta.1",
  "description": "A simple graphql admin dashboard. Easy to expand with the flexible plugin. Graphql first, also supports api. Based on React, Redux, Material-UI, Realtime RxDB, Formik, I18N.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bunred/bunadmin"
  },
  "bugs": {
    "url": "https://github.com/bunred/bunadmin/issues"
  },
  "author": "BunRed Team",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "files": [
    "lib",
    "plugin.js"
  ],
  "scripts": {
    "setup": "yarn && yarn build",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write  --no-semi src/** pages/** plugins/** test/**",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "ttsc": "rm -rf lib & ttsc -p tsconfig.pack.json",
    "ttsc-pack": "rm -rf lib & ttsc -p tsconfig.pack.json && npm pack"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.54",
    "@mdx-js/loader": "^1.6.5",
    "@reduxjs/toolkit": "^1.3.6",
    "@types/mockjs": "^1.0.3",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-redux": "^7.1.9",
    "babel-loader": "^8.1.0",
    "clsx": "^1.1.1",
    "filehound": "^1.17.4",
    "formik": "^2.1.4",
    "formik-material-ui": "^2.0.0-beta.1",
    "i18next": "^19.4.5",
    "json-bigint": "^0.3.0",
    "material-table": "npm:@bunred/material-table@0.69.0-beta.1",
    "mockjs": "^1.1.0",
    "notistack": "^0.9.16",
    "pouchdb-adapter-http": "^7.2.1",
    "pouchdb-adapter-idb": "^7.2.1",
    "pouchdb-adapter-memory": "^7.2.1",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-dropzone": "^11.2.0",
    "react-eva-icons": "0.0.8",
    "react-i18next": "^11.5.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "rxdb": "8.9.0",
    "rxjs": "^6.5.4",
    "shortid": "^2.2.15",
    "umi-request": "^1.3.3"
  },
  "devDependencies": {
    "@types/mdx-js__react": "^1.5.2",
    "@types/node": "^14.0.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "customize-cra": "^1.0.0",
    "husky": "^4.2.5",
    "ignore-loader": "^0.1.2",
    "jest": "^25.5.4",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "react-app-rewire-alias": "^0.1.6",
    "react-app-rewired": "^2.1.6",
    "serve-handler": "^6.1.3",
    "ttypescript": "^1.5.12",
    "typescript": "^3.9.3",
    "typescript-transform-paths": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "admin",
    "graphql",
    "react",
    "react admin",
    "material",
    "material admin",
    "plugin",
    "dashboard",
    "admin panel"
  ],
  "homepage": "https://www.bunred.com/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
